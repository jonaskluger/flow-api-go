# Flow Production Tracking API Configuration
# Copy this file to .env and fill in your actual credentials

# Your Flow site URL
FLOW_SITE_URL=https://myproject.shotgrid.autodesk.com

# Your API Script Name (from Admin > Scripts)
FLOW_SCRIPT_NAME=your_script_name

# Your API Script Key/Application Key (from Admin > Scripts)
FLOW_SCRIPT_KEY=your_script_key_here

# Your Project ID (optional - can be found in Flow URL or by getting projects)
FLOW_PROJECT_ID=123
